     yaxis: [\n                        {\n                            y: 600,\n                            borderColor: '#00E396',\n                            label: {\n                                borderColor: '#00E396',\n                                style: {\n                                    color: '#111111',\n                                    background: '#dadada'\n                                },\n                                text: 'Capacidade do sistema de saúde'\n                            }\n                        }\n                    ]\n                },\n                stroke: {\n                    curve: 'smooth',\n                },\n                colors: [ '#2E93fA', '#c21f1f' ],\n                chart: {\n                    id: 'vuechart-example'\n                },\n                xaxis: {\n                    categories: this.time\n                }\n            },\n            series: [\n                {\n                    name: 'Infectados',\n                    data: this.infected\n                },\n                {\n                    name: 'Óbitos',\n                    data: this.death\n                }\n            ],\n            numId: 0\n        }\n    },\n    created(){\n        this.reduce_data(this.time)\n    },\n    methods: {\n        reduce_data(time){\n            this.time = time.map(function(time){\n                return new Date(time).toISOString().split('T')[0].slice(5)\n            })\n        }\n    }\n}\n</script>\n\n<style scoped>\n#RA_name{\n    margin-top: 15px;\n    margin-bottom: 15px;\n    font-size: 25px;\n}\n\n.selector{\n    align-content: center;\n    text-align: center;\n}\n</style>"]}]}