Content":["<template>\n    <v-card flat color=\"grey lighten-4\">\n        <v-card-text>\n            <v-row>\n                <v-col class=\"px-4\">\n                <v-range-slider\n                    v-model=\"range\"\n                    :max=\"max\"\n                    :min=\"min\"\n                    height=\"0\"\n                    hide-details\n                    class=\"align-center\"\n                    :key=\"numId\"\n                    v-on:mouseup=\"sendRange()\"\n                />\n                </v-col>\n            </v-row>\n        </v-card-text>\n        <v-subheader> <h2>De: {{displayDates(range[0])}}</h2> <v-spacer/> <h2>At√©: {{ displayDates(range[1])}}</h2> </v-subheader>\n    </v-card>\n</template>\n\n\n<script>\n    export default {\n        props: [ 'dates' ],\n        data () {\n            return {\n                min: null,\n                max: null,\n                range: [],\n                dateRange: [],\n                numId: 0,\n            }\n        },\n        mounted(){\n            this.setSlider()\n        },\n        watch: {\n            dates(val){\n                let min = 0\n                let max = val.length -1\n                this.range = [min,max]\n                this.min = min\n                this.max = max\n            },\n        },\n        methods: {\n            displayDates(item){\n                return this.dates[item].split(\"T\")[0]\n            },\n            sendRange(){\n                this.$emit('changeRange', this.range)\n            }\n        }\n    }\n</script>"],"sourceRoot":"src/views/Projeto/components"}]}