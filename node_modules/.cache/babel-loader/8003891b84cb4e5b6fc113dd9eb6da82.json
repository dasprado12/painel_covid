A,CAAA","sourcesContent":["<template>\n    <div id=\"app\">\n        <l-map :center=\"[-23.752961, -57.854357]\" :zoom=\"6\" style=\"height: 500px;\" :options=\"mapOptions\">\n        <l-choropleth-layer :data=\"pyDepartmentsData\" titleKey=\"department_name\" idKey=\"department_id\" :value=\"value\" :extraValues=\"extraValues\" geojsonIdKey=\"dpto\" :geojson=\"geojson\" :colorScale=\"colorScale\">\n            <template slot-scope=\"props\">\n            <l-info-control :item=\"props.currentItem\" :unit=\"props.unit\" title=\"Department\" placeholder=\"Hover over a department\"/>\n            <l-reference-chart title=\"Girls school enrolment\" :colorScale=\"colorScale\" :min=\"props.min\" :max=\"props.max\" position=\"topright\"/>\n            </template>\n        </l-choropleth-layer>\n        </l-map>\n    </div>\n</template>\n\n<script>\nimport { InfoControl, ReferenceChart, ChoroplethLayer } from 'vue-choropleth'\n\nimport { LMap } from 'vue2-leaflet';\n    export default {\n    name: \"app\",\n    components: { \n        LMap,\n        'l-info-control': InfoControl, \n        'l-reference-chart': ReferenceChart, \n        'l-choropleth-layer': ChoroplethLayer \n    },\n    data() {\n        return {\n        colorScale: [\"e7d090\", \"e9ae7b\", \"de7062\"],\n        value: {\n            key: \"amount_w\",\n            metric: \"% girls\"\n        },\n        extraValues: [{\n            key: \"amount_m\",\n            metric: \"% boys\"\n        }],\n        mapOptions: {\n            attributionControl: false\n        },\n        currentStrokeColor: '3d3213'\n        }\n    },\n    async created() {\n        this.created();\n    },\n    methods: {\n        async create(){\n            const response = await fetch('https://raw.githubusercontent.com/dasprado12/Brasilia-RAs-georreferenciadas/master/Geojsons/All.geojson');\n            this.geojson = await response.json() \n        }\n    }\n    }\n</script>\n<style>\n@import \"../node_modules/leaflet/dist/leaflet.css\";\nbody {\n  background-color: #e7d090;\n  margin-left: 100px;\n  margin-right: 100px;\n}\n#map {\n  background-color: #eee;\n}\n</style>"],"sourceRoot":"src/views/Projeto/maps"}]}