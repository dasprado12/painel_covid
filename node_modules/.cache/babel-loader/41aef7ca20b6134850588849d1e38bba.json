itos += this.dados[j].obitos\n                    }\n                }\n                this.filtrado_inf.data.push(infectados)\n                this.filtrado_obt.data.push(obitos)\n            }\n            this.filtrado_inf.time = this.dates\n            this.filtrado_obt.time = this.dates\n        }\n    }\n}\n</script>"],"sourceRoot":"src/views/Report/components"}]}