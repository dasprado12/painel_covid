A,KAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AANA;AApBA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <v-card flat outlined> \r\n            <v-card-title>\r\n                <span class=\"font-weight-light\">Óbitos</span>\r\n            </v-card-title> <v-divider/>\r\n            <line-chart\r\n                v-bind:time=\"time\"\r\n                v-bind:data=\"data\"\r\n                :key=\"numId\"\r\n            />\r\n        </v-card>\r\n        <span class=\"font-weight-light\">Histórico</span>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport lineChart from \"../charts/MixLineChartObt\"\r\nimport { Data } from \"../../../functions/index.js\";\r\n\r\nlet api_data = new Data();\r\n\r\nexport default {\r\n    props: [ 'type', 'regions' ],\r\n    components: { lineChart },\r\n    data(){\r\n        return {\r\n            time: [],\r\n            data: null,\r\n            items: [],\r\n            numId: 0\r\n        }\r\n    },\r\n    async mounted(){    \r\n        this.get_many_data()\r\n    },\r\n    watch: {\r\n        regions(val){\r\n            let string = val.toString()\r\n            this.get_many_data(string)\r\n        }\r\n    },\r\n    methods: {\r\n        async get_many_data(string){\r\n            this.items = (await api_data.get_all_regions()).data\r\n            this.data = (await api_data.get_many_data('obitos', string)).data\r\n            this.time = (await api_data.get_all_dates()).data.map(function(item){ return item.split(\"T\")[0] })\r\n            this.numId++\r\n        }\r\n    }       \r\n}\r\n</script>"],"sourceRoot":"src/views/Projeto/components"}]}